{
  "name": "eliza-town",
  "version": "0.1.0",
  "description": "A visual multi-agent orchestration system powered by Eliza OS",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "db:setup": "psql -U postgres -f src/db/schema.sql",
    "db:reset": "psql -U postgres -c 'DROP DATABASE IF EXISTS eliza_town' && psql -U postgres -c 'CREATE DATABASE eliza_town' && npm run db:setup",
    "lint": "eslint src/",
    "test": "node --test"
  },
  "keywords": [
    "eliza-os",
    "multi-agent",
    "ai-agents",
    "orchestration",
    "three-js",
    "visualization",
    "autonomous-agents",
    "claude",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/eliza-town"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "archiver": "^7.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pg": "^8.12.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
